<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.costumview.MainActivity">

    <com.example.costumview.EdtiTextDude
        android:layout_margin="20dp"
        android:id="@+id/editTextDude"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <com.example.costumview.ButtonDude
        android:layout_below="@id/editTextDude"
        android:id="@+id/buttonDude"
        android:layout_margin="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:enabled="false"/>

</RelativeLayout>